{
    "name": "laravel/laravel",
    "type": "project",
    "description": "The skeleton application for the Laravel framework.",
    "keywords": [
        "laravel",
        "framework"
    ],
    "license": "MIT",
    "require": {
        "php": "^8.1",
        "acme/analytics": "@dev",
        "alexrubl/date-range": "@dev",
        "alexrubl/time-range": "@dev",
        "alexrubl/video": "@dev",
        "coderello/laravel-nova-lang": "^2.0",
        "formfeed-uk/nova-dependable-panel": "^1.2",
        "ganyicz/nova-callbacks": "^1.1",
        "guzzlehttp/guzzle": "^7.2",
        "inertiajs/inertia-laravel": "^0.6.11",
        "laravel/framework": "^10.10",
        "laravel/nova": "*",
        "laravel/sanctum": "^3.2",
        "laravel/tinker": "^2.8",
        "norman-huth/nova-radio-field": "^1.1",
        "outl1ne/nova-settings": "^5.2",
        "pavloniym/nova-action-buttons": "^1.1",
        "pbmedia/laravel-ffmpeg": "^8.3",
        "php-junior/nova-logs": "^1.1",
        "simplesquid/nova-advanced-number-field": "^3.1",
        "symfony/process": "^6.3",
        "test/test": "@dev",
        "vyuldashev/nova-permission": "^3.2"
    },
    "require-dev": {
        "fakerphp/faker": "^1.9.1",
        "laracasts/generators": "^2.0",
        "laravel/pint": "^1.0",
        "laravel/sail": "^1.18",
        "mockery/mockery": "^1.4.4",
        "nunomaduro/collision": "^7.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ignition": "^2.0"
    },
    "autoload": {
        "psr-4": {
            "App\\": "app/",
            "Database\\Factories\\": "database/factories/",
            "Database\\Seeders\\": "database/seeders/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        },
        "files": [
            "app/Helpers/helpers.php"
        ]
    },
    "scripts": {
        "post-autoload-dump": [
            "Illuminate\\Foundation\\ComposerScripts::postAutoloadDump",
            "@php artisan package:discover --ansi"
        ],
        "post-update-cmd": [
            "@php artisan vendor:publish --tag=laravel-assets --ansi --force"
        ],
        "post-root-package-install": [
            "@php -r \"file_exists('.env') || copy('.env.example', '.env');\""
        ],
        "post-create-project-cmd": [
            "@php artisan key:generate --ansi"
        ],
        "post-install-cmd": [
            "php artisan storage:link --quiet"
        ]
    },
    "extra": {
        "laravel": {
            "dont-discover": []
        }
    },
    "config": {
        "optimize-autoloader": true,
        "preferred-install": "dist",
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "repositories": [
        {
            "type": "path",
            "url": "./nova"
        },
        {
            "type": "path",
            "url": "./nova-components/Analytics"
        },
        {
            "type": "path",
            "url": "./nova-components/Test"
        },
        {
            "type": "path",
            "url": "./nova-components/Test"
        },
        {
            "type": "path",
            "url": "./nova-components/Test"
        },
        {
            "type": "path",
            "url": "./nova-components/Test"
        },
        {
            "type": "path",
            "url": "./nova-components/Video"
        },
        {
            "type": "path",
            "url": "./nova-components/DateRange"
        },
        {
            "type": "path",
            "url": "./nova-components/TimeRange"
        }
    ],
    "minimum-stability": "stable",
    "prefer-stable": true    
}
